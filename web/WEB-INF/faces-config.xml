<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>adminPage</from-outcome>
            <to-view-id>/faces/adminPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
       
    <navigation-rule>
        <navigation-case>
            <from-outcome>managePage</from-outcome>
            <to-view-id>/faces/managePage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>userPage</from-outcome>
            <to-view-id>/faces/userPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/faces/newjsf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
</faces-config>
